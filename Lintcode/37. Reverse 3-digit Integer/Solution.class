public class Solution {
    /**
     * @param number: A 3-digit number.
     * @return: Reversed number.
     */
    public int reverseInteger(int number) {
        int answer = 0;
        int len = number.length;

        for (int i = 0; i < len; i++){
            answer = answer + ((number % 10) * (10 ** (n - i - 1)));
            number = number / 10;
        }

        return answer;
    }

    public void test(){
        System.out.println(reverseInteger(239));
    }

    
}